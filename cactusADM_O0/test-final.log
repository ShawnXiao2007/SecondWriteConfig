mkdir temp
cd temp
CYCLES=13
starttime=$(date +%s)
for((i=0; i<$CYCLES;))
do
	../../original_binaries/cactusADM_O0 ../../CPU2006/436.cactusADM/data/test/input/benchADM.par > log_in 
	i=$(expr $i + 1)
done
endtime1=$(date +%s)

for((i=0; i<$CYCLES;))
do
	../../output_binaries/cactusADM_O0.out ../../CPU2006/436.cactusADM/data/test/input/benchADM.par > log_out
	i=$(expr $i + 1)
done

endtime2=$(date +%s)
echo "cactusADM original $((endtime1-starttime)) s; instrumented $((endtime2-endtime1)) s"

if diff log_in log_out >/dev/null 2>&1
then 
	printf "CACTUSADM_O0 \t \t Success \n "
else
	printf "CACTUSADM_O0 \t \t Failed \n "
fi
rm log_in log_out
cd ../
rm -rf temp
